  author       = {Marco Magnani and Paolo Puliti and Mario Stefanon},
  title        = {Numerical solution of the inverse problem in the analysis of neutron small angle scattering experiments},
  journal      = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
  journaltitle = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
