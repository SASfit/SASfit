@Article{Glatter1977,
  author       = {Glatter, O.},
  title        = {{A new method for the evaluation of small-angle scattering data}},
  journaltitle = {Journal of Applied Crystallography},
  date         = {1977-10},
  volume       = {10},
  number       = {5},
  pages        = {415--421},
  doi          = {10.1107/S0021889877013879},
  abstract     = {A new numerical method is presented for simultaneous smoothing, desmearing
	and Fourier transformation of X-ray and neutron small-angle scattering
	data. The method can only be applied to scattering curves from dilute
	particle systems, {\it i.e.} for scattering media whose distance
	distributions are zero beyond a certain value. The distance distribution
	of the scattering medium is approximated by a linear combination
	of about 20 to 30cubic {\it B}-splines. These spline functions have
	a restricted extension in real space. Their coefficients are adjusted
	by a weighted least-squares operation so that the series, after being
	Fourier transformed and smeared according to the geometry and wavelength
	distribution, represents an optimum smoothed approximation of the
	experimental data. Tendencies towards oscillations in the least-squares
	operation are suppressed by a new stabilization routine. The method
	offers a new possibility for the estimation of the radius of gyration,
	which is generally superior to the Guinier approximation.},
  owner        = {kohlbrecher},
  timestamp    = {2016.04.02},
}

@Article{Vonk1976,
  author       = {Vonk, C. G.},
  title        = {{On two methods of determination of particle size distribution functions by means of small-angle X-ray scattering}},
  journal      = {Journal of Applied Crystallography},
  journaltitle = {Journal of Applied Crystallography},
  year         = {1976},
  date         = {1976-12},
  volume       = {9},
  number       = {6},
  pages        = {433--440},
  doi          = {10.1107/S0021889876011874},
  abstract     = {The total X-ray intensity as a function of {\it h} ({\it h} is the
	radial coordinate in reciprocal space), scattered by an isotropic
	system of particles of equal shapes but of different sizes {\it R},
	can, under certain conditions, be expressed as an integral over the
	particle size distribution function {\it D}({\it R}), multiplied
	by a common single-particle function of {\it hR} which can be calculated
	from the assumed particle shape. In the first method {\it D}({\it
	R}) is calculated from this relation by the method of least squares,
	in which values of {\it D} at a limited number of particle sizes
	are the unknowns. To avoid oscillations in the {\it D} curve, constraints
	are imposed on the {\it D} values. The proper weight to be assigned
	to these constraints must be determined by trial and error. The method
	has been adapted to suit various assumptions and requirements as
	to the shape of the particles, the type of distribution function
	to be calculated, and experimental conditions (slit or pinhole focusing).
	The second method is essentially the one described by Schmidt, Weil
	& Brill [{\it X-ray & Electron Methods of Analysis}, pp. 86{--}100.
	(1968), New York: Plenum], which, however, is adapted to the use
	of slit-smeared intensities. Both methods may give rise to artefacts
	in the calculated distribution functions in the range of the smallest
	particle sizes, which are sensitive to the setting of the various
	parameters and to experimental errors. However, the position and
	shape of the main maxima can usually be determined quite well. The
	agreement between the results obtained by the two methods is satisfactory.},
  owner        = {kohlbrecher},
  timestamp    = {2016.04.03},
}

