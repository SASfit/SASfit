@Article{Richter1997,
  author    = {D. Richter and D. Schneiders and M. Monkenbusch and L. Willner and L. J. Fetters and J. S. Huang and M. Lin and K. Mortensen and B. Farago},
  title     = {Polymer Aggregates with Crystalline Cores:~ The System Polyethylene-Poly(ethylenepropylene)},
